{
  "presets": [
    [
      "latest", {
        "es2015": {
          "modules": false
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread",
    "transform-function-bind",
    ["transform-class-properties", { "loose": true }]
  ],
  "env": {
      "test": {
          "plugins": ["transform-es2015-modules-commonjs"]
      }
  }
}
